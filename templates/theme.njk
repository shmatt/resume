<body class="{{ basics.name | slugify }}">
  <article>
    <section class="basics">
      <img src="{{ basics.image }}" alt="{{ basics.name }}" class="picture">
      <h1>{{ basics.name }}</h1>
      <h2>{{ basics.label }}</h2>
      <p class="summary">{{ basics.summary }}</p>
      <address>
        <ul>
          <li class="location">{{ basics.location.city }}, {{ basics.location.region }}</li>
          <li class="phone"><a href="tel:{{ basics.phone }}">{{ basics.phone }}</a></li>
          <li class="email"><a href="mailto:{{ basics.email }}">{{ basics.email }}</a></li>
          <li class="url"><a href="{{ basics.url }}">{{ basics.url }}</a></li>
        </ul>
      </address>
      <dl class="profiles">
        {% for profile in basics.profiles %}
        <div class="{{ profile.network | slugify }}">
          <dt class="network">{{ profile.network }}</dt>
          <dd class="username"><a href="{{ profile.url }}" class="url">{{ profile.username }}</a></dd>
        </div>
        {% endfor %}
      </ul>
    </section>

    <section class="skills">
      <h2>Skills</h2>
      <ul>
        {% for skill in skills %}
        <li class="{{ skill.name | slugify }} {{ skill.level | slugify }}">
          <h3 class="name">{{ skill.name }}</h3>
          <span class="level">{{ skill.level }}</span>
          <ul class="keywords">
            {% for keyword in skill.keywords %}
            <li>{{ keyword }}</li>
            {% endfor %}
          </ul>
        </li>
        {% endfor %}
      </ul>
    </section>

    <section class="work">
      <h2>Work</h2>
      <ul>
        {% for job in work %}
        <li class="{{ job.name | slugify }}">
          <h3 class="name"><a href="{{job.url}}" class="url">{{ job.name }}</a></h3>
          {% if job.startDate or job.endDate %}
          <div class="dates">
            {% if job.startDate %}<span class="startDate">{{ job.startDate }}</span>{% endif %}
            {% if job.startDate %} - {% endif %}
            <span class="endDate">
              {% if job.endDate %}
              {{ job.endDate }}
              {% else %}
              Current
              {% endif %}
            </span>
          </div> 
          {% endif %}
          <span class="location">{{ job.location }}</span>
          <span class="position">{{ job.position }}</span>
          <p class="description">{{ job.description }}</p>
          <ul class="highlights">
            {% for highlight in job.highlights %}
            <li>{{ highlight }}</li>
            {% endfor %}
          </ul>
        </li>
        {% endfor %}
      </ul>
    </section>
    
    <section class="Projects">
      <h2>Projects</h2>
      <ul>
        {% for project in projects %}
        <li class="{{ project.name | slugify }}">
          <h3 class="name"><a href="{{ project.url}}" class="url">{{ project.name }}</a></h3>
          {% if project.startDate or project.endDate %}
          <div class="dates">
            {% if project.startDate %}<span class="startDate">{{ project.startDate }}</span>{% endif %}
            {% if project.startDate %} - {% endif %}
            <span class="endDate">
              {% if project.endDate %}
              {{ project.endDate }}
              {% else %}
              Current
              {% endif %}
            </span>
          </div> 
          {% endif %}
          <p class="summary">{{ project.summary }}</p>
          <ul class="highlights">
            {% for highlight in project.highlights %}
            <li>{{ highlight }}</li>
            {% endfor %}
          </ul>
        </li>
        {% endfor %}
      </ul>
    </section>
    
    <section class="interests">
      <h2>Interests</h2>
      <ul>
        {% for interest in interests %}
        <li class="{{ interest.name | slugify }}">
          <h3 class="name">{{ interest.name }}</h3>
          <ul class="keywords">
            {% for keyword in interest.keywords %}
            <li>{{ keyword }}</li>
            {% endfor %}
          </ul>
        </li>
        {% endfor %}
      </ul>
    </section>

  </article>

  <footer>
    {% if basics.name == "Matt Tew" %}
    <div>
      The template for this document was wholly created by <a href="{{ basics.url }}">Matt Tew</a> and can be viewed on <a href="https://github.com/shmatt/resume">GitHub</a>. 
    </div>
    {% endif %}
  </footer>
</body>

</html>