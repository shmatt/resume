  <title>Resume of {{ basics.name }}</title>

  <style>

    @import url('https://fonts.googleapis.com/css2?family=Fira+Code:wght@400;700&family=Fira+Sans:ital,wght@0,400;0,700;1,400;1,700&family=Share+Tech&display=swap'); 

    html {
      font-size: 16px;
      --primary: #fff;
      --accent: #c87333;
      --background: #000;
      --alt-background: #888;
      --heading-family: 'Share Tech', sans-serif;
      --body-family: 'Fira Sans', sans-serif;
      --fa-primary-color: #fff;
      --fa-primary-opacity: 1;
      --fa-secondary-color: #c87333;
      --fa-secondary-opacity: 1;
    }

    @media print {
      html {
        font-size: 12px;
        --primary: #000;
        --accent: #c87333;
        --background: #fff;
        --alt-background: #888;
        --fa-primary-color: #000;
      }
      
      body {
        -webkit-print-color-adjust:exact !important;
        print-color-adjust:exact !important;
      }

    }

    body {
      background: var(--background);
      color: var(--primary);
      font-family: var(--body-family);
      font-size: 1.2em;
      line-height: 1.5em;
      margin: 0;
      padding: 0;
    }

    article {
      max-width: 1024px;
    }

    h1, h2, h3 {
      font-family: var(--heading-family);
      color: var(--primary);
    }

    a {
      color: var(--accent);
      text-decoration: none;
    }

    address {
      font-style: normal;
    }

    ul, ol {
      margin: 0;
      padding: 0;
      list-style: none;
    }

    ul > li, ol > li {
      margin: 0;
      padding: 0;
    }

    section {
      position: relative;
      margin-left: 5rem;
      border-left: 2px solid var(--accent);
      padding-left: 2rem;
    }

    .basics img {
      position: absolute;
      top: 2em;
      left: -4rem;
      width: 3rem;
      height: 3rem;
      border-radius: 50%;
    } 

    .basics address li {
      margin-bottom: 1em;
      position: relative;
    }

    .basics .profiles > div {
      margin-bottom: 1em;
      position: relative;
      break-inside: avoid;
    }

    .basics .profiles dd {
      margin: 0;
    }

    .skills .level {
      display: none;
    }

    .interests > ul > li,
    .skills > ul > li {
      position: relative;
      break-inside: avoid;
    }

    .interests .keywords,
    .skills .keywords {
      position: relative;
      display: flex;
      flex-wrap: wrap;
      gap: .5rem;
      justify-content: flex-start;
      align-items: flex-start;
    }

    .interests .keywords > li,
    .skills .keywords > li {
      font-size: .8rem;
      font-weight: 700;
      text-transform: uppercase;
      padding: 0.25rem 0.5rem;
      line-height: 1rem;
      border-radius: 0.5rem;
    }

    .skills .expert .keywords > li {
      background: var(--accent);
      color: var(--primary);
    }

    .interests .keywords > li,
    .skills .competent .keywords > li {
      background: var(--alt-background);
      color: var(--primary);
    }

    @media print {
      
      .interests .keywords > li,
      .skills .competent .keywords > li,
      .skills .expert .keywords > li {
        color: var(--background);
      }
    }

    .basics .profiles > div:before,
    .basics .profiles > div:after,
    .basics address li:before,
    .basics address li:after,
    .interests > ul > li:before,
    .interests > ul > li:after,
    .skills > ul > li:before,
    .skills > ul > li:after,
    .skills .keywords:before,
    .skills .keywords:after {
      display: flex;
      align-items: center;
      justify-content: center;
      position: absolute;
      font-style: normal;
      font-variant: normal;
      text-rendering: auto;
      font-family: "Font Awesome 6 Duotone";
      font-weight: 900;
      font-size: 1.5rem;
      -webkit-font-smoothing: antialiased;
      top: 0.4rem;
      left: -5.5rem;
      width: 1.5rem;
      height: 1.5rem;
      margin: 0;
    }

    .basics .profiles > div:before,
    .basics address li:before,
    .interests > ul > li:before,
    .skills > ul > li:before,
    .skills .keywords:before {
      color: var(--fa-primary-color, inherit);
      opacity: 1;
      opacity: var(--fa-primary-opacity, 1.0);
    }

    .basics .profiles > div:after,
    .basics address li:after,
    .interests > ul > li:after,
    .skills > ul > li:after,
    .skills .keywords:after {
      color: var(--fa-secondary-color, inherit);
      opacity: 1;
      opacity: var(--fa-secondary-opacity, 1);
